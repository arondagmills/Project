doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css' rel='stylesheet')
  link(rel='stylesheet' href='css/farmer1.css')
  title Agriculture Officer
section
  .navbar-dark
    nav.navbar.navbar-expand-lg.navbar-light.text-bg-light
      .container-fluid
        button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarTogglerDemo01' aria-controls='navbarTogglerDemo01' aria-expanded='false' aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarTogglerDemo01.collapse.navbar-collapse
          a.navbar(href='/')
            b U
          | farm
          ul.navbar-nav.me-auto.mb-2.mb-lg-0
            li.nav-item
              a.nav-link.active(aria-current='page' href='#') Products
            li.nav-item
              a.nav-link(href='#') About us
            li.nav-item
              a.nav-link(href='#' tabindex='-1' aria-disabled='true') Sign Up
  section(style='margin: 4%;')
    .d-flex.align-items-start
      #v-pills-tab.nav.flex-column.nav-pills.me-3(role='tablist' aria-orientation='vertical')
        button#v-pills-home-tab.nav-link.active(data-bs-toggle='pill' data-bs-target='#v-pills-home' type='button' role='tab' aria-controls='v-pills-home' aria-selected='true') Dashboard
        button#v-pills-profile-tab.nav-link(data-bs-toggle='pill' data-bs-target='#v-pills-profile' type='button' role='tab' aria-controls='v-pills-profile' aria-selected='false') Register
        button#v-pills-disabled-tab.nav-link(data-bs-toggle='pill' data-bs-target='#v-pills-disabled' type='button' role='tab' aria-controls='v-pills-disabled' aria-selected='false')
          | Farmer one List
      #v-pills-tabContent.tab-content
        #v-pills-home.tab-pane.fade.show.active(role='tabpanel' aria-labelledby='v-pills-home-tab' tabindex='0')
        #v-pills-profile.tab-pane.fade(role='tabpanel' aria-labelledby='v-pills-profile-tab' tabindex='0')
          form(action="/OA" method="post") 
            h1 Registration of Farmerone
            .row
              .col-3
                label.form-label(for='Name1') Name
                input#Name1.form-control(type='String' name= "Name1" aria-describedby='nameHelp')
                #nameHelp.form-text
              .col-3
                label.form-label(for='Ward1') Ward
                select#Ward1.form-control(input='ward' type='String' name="ward1" aria-describedby='wardHelp')
                  option(value='0')
                  option(value='Ward1') Ward1
                  option(value='Ward2') Ward2
                  option(value='Ward3') Ward3
                  option(value='Ward4') Ward4
              .col-3
                label.form-label(for='Date1') Date of Registration
                input#date1.form-control(type='date' name="Date1" aria-describedby='date1Help')
                #date1Help.form-text
              .col-3
                label.form-label(for='UserID') Unique
                input#UserID.form-control(type='UserID' name="UserID" aria-describedby='UserIDHelp' placeholder='FO1')
                #userIDHelp.form-text
              .col-3
                label.form-label(for='Gender1') Gender
                select#gender1.form-control(input='gender' type='String' name="Gender1" aria-describedby='genderHelp')
                  option(value='0')
                  option(value='Male') Male
                  option(value='Female') Female
              .col-3
                label.form-label(for='Date2') Date of birth
                input#date2.form-control(type='date' name="Date2" aria-describedby='date2Help')
                #date2Help.form-text
              .col-3
                label.form-label(for='activities1') Activities
                select#activities1.form-control(input='activities' type='String' name="Activities" aria-describedby='activitiesHelp')
                  option(value='0')
                  option(value='Farming') Farming
                  option(value='Marketing') Marketing
              .col-3
                label.form-label(for='Nin') Nin
                input#nin.form-control(type='String' name="Nin" aria-describedby='ninHelp' placeholder='ID')
                #ninHelp.form-text
              .col-3
                label.form-label(for='Direction') Direction of Residence
                input#dierction.form-control(type='String' name="Direction" aria-describedby='directionHelp')
                #directionHelp.form-text
              .col-2
                label.form-label(for='role') Role 
                select#role1.form-control(input='Role' type='String' name="role" aria-describedby='roleHelp')
                  option(value='0')
                  option(value='Agriculture Officer') Agriculture Officer
                  option(value='Farmer one') Farmer one
                  option(value='Urban farmer') Urban farmer
              .col-3
                label.form-label(for='Direction1') Residence type
                input#dierction1.form-control(type='String' name="direction1" aria-describedby='direction1Help')
                #direction1Help.form-text
              .col-3
                label.form-label(for='Password') Password
                input#password.form-control(type='password' name="password" aria-describedby='passwordHelp')
                #passwordHelp.form-text
              .col-2
                label.form-label(for='Ward2') Ward btn 1-10 years
                select#Ward2.form-control(input='ward2' type='String' name="ward2" aria-describedby='wardHelp')
                  option(value='0')
                  option(value='Ward1') Ward1
                  option(value='Ward2') Ward2
                  option(value='Ward3') Ward3
                  option(value='Ward4') Ward4
              .col-3
                button.btn.btn-primary(type='submit' style='margin: 10px;') Register
        #v-pills-disabled.tab-pane.fade(role='tabpanel' aria-labelledby='v-pills-disabled-tab' tabindex='0' style='text-align: center')
          form(action='/OA' method='post')
            h1 Farmer ones
            .row
              .col-md-6
                table.table
                  thead
                    tr
                      th(scope='col') Names
                      th(scope='col') Ward
                      th(scope='col') Phone number
                      th(scope='col') Gender
                      th(scope='col') Update
                      th(scope='col') Delete
                  tbody
                    tr
                      th(scope='row') Name1
                      td Ward1
                      td phonenumber
                      td Gender1
                      td
                        a.btn.btn-success(href='') Update
                      td
                        a.btn.btn-danger(href='') Delete
    .row
      .col-6
        article
          img(src='image/pexels-helena-lopes-4731089.jpg' width='100%' alt='')
          p
      .col-6
        article
          img(src='image/black guy.jpg' width='100%' alt='')
  div(style='margin-bottom: 25%;')
  section(style='nav-up: inherit;')
    footer(style='text-align: center; background-color: rgb(63, 61, 61)')
      p
        strong Follow us
      img(src='image/3225179_media_app_social_popular_logo_whatsapp.png' style='width: 50px' alt='')
      img(src='image/3225194_app_logo_popular_social_facebook_media.png' style='width: 50px' alt='')
      img(src='image/3225180_logo_popular_social_youtube_app_media.png' style='width: 50px' alt='')
      img(src='image/3225191_social_instagram_media_app_popular_logo.png' style='width: 50px' alt='')
  script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js')
